<!DOCTYPE html>
<html lang="en">

<main>

    <ul>
      <% allNotes.forEach(note => { %>
      <li id="<%= note.id %>" class="<%= note.authorid %>">
        <p><%= note.type %></p>
        <p><%= note.note %></p>
  
        <form action="/notes/<%= note.id %>?_method=put" method="POST">
          <input type="hidden" name="note" value="<%= note.note %>">
          <input type="submit" value="Edit Note">
        </form>
        
        <form id="delete" action="/notes/<%= note.id %>?_method=delete" method="POST">
          <input type="hidden" name="id" value="<%= note.id %>">
          <input type="submit" value="Delete Note">
        </form>
      </li>
      <%})%>
      </ul>
      
      
      
